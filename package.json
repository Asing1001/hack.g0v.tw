    "streamqueue": "0.0.6",
    "gulp-uglify": "^0.2.1",
    "gulp-concat": "^2.2.0",
    "gulp-bower-files": "^0.2.1",
    "gulp-filter": "^0.3.1",
    "gulp-bower": "0.0.3",
    "gulp-livereload": "^1.2.0",
