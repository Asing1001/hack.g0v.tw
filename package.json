    "static-jade-brunch": ">= 1.4.8 < 1.5",
    "auto-reload-brunch": "1.6.x",
    "uglify-js-brunch": "1.5.x",
    "clean-css-brunch": "1.5.x",
    "jade-angularjs-brunch": "0.0.5",
    "jsenv-brunch": "1.4.2",
    "karma": "~0.10.5",
    "karma-live-preprocessor": "~0.2.0",
    "karma-mocha": "~0.1.0",
    "karma-chai": "~0.0.2",
