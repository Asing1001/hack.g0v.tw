    "bower": "1.2.x",
    "mocha": "~1.14.0",
    "chai": "~1.8.0",
    "gulp-livereload": "~1.2.0",
    "connect-livereload": "~0.3.2",
    "gulp-ruby-sass": "~0.4.1",
    "tiny-lr": "0.0.5",
    "gulp-util": "~2.2.14",
    "gulp": "~3.6.0",
    "gulp-jade": "~0.4.2",
    "express": "~3.5.1",
    "gulp-insert": "~0.2.0",
